{
  "name": "express-auth-scaffolding",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clear": "jest --clearCache && rm -rf build",
    "lint": "eslint --ext .ts,.js --ignore-path .gitignore .",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix",
    "lint:js": "eslint --ext \".js,.ts\" --ignore-path .gitignore .",
    "dev": "TZ=UTC ts-node-dev --respawn --exit-child --watch .env --transpile-only src/server.ts",
    "build": "tsc -b tsconfig.build.json",
    "start": "TZ=UTC node build/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migration": "TZ=UTC ts-node src/migrations/index.ts",
    "prod:migration": "TZ=UTC node build/migrations/index.js",
    "seed": "TZ=UTC ts-node src/scripts/seed.ts",
    "prod:seed": "TZ=UTC node build/scripts/seed.js"
  },
  "keywords": [
    "express",
    "typescript",
    "authentication",
    "scaffolding"
  ],
  "author": "",
  "license": "ISC",
  "description": "Express TypeScript scaffolding with authentication",
  "dependencies": {
    "@getbrevo/brevo": "^2.2.0",
    "@types/multer": "^1.4.12",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "eslint": "^8.20.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.1.0",
    "mongo-migrate-ts": "^1.6.2",
    "mongodb": "^6.14.2",
    "multer": "^1.4.5-lts.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node-dev": "^2.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mongodb": "^4.0.6",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^22.13.10",
    "@types/request": "^2.48.12",
    "@types/supertest": "^6.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}